{
  "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react",
      "lib": [
          "es6"
      ],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "target": "esnext",
      "baseUrl": ".",
      "paths": {
          "screens/*":["./src/screens/*"],
          "components/*":["./src/components/*"],
          "global/*":["./src/global/*"],
          "assets/*":["./src/assets/*"],
          "navigation/*":["./src/navigation/*"],
          "modals/*":["./src/modals/*"],
          "reduxtoolkit/*":["./src/redux/*"]
    }
  },
  "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
  ]
}